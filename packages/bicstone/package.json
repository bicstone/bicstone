		"esbuild": "0.19.8",
		"typescript": "5.2.2"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"files": ["LICENSE", "dist"],
	"homepage": "https://github.com/bicstone/bicstone#readme",
	"keywords": [
		"portfolio",
		"javascript",
		"personal",
		"hacktoberfest",
		"npm card",
		"npx card",
		"business card"
	],
	"license": "CC0-1.0",
	"main": "./dist/index.js",
	"name": "bicstone",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bicstone/bicstone.git"
	},
	"scripts": {
		"build": "npm run build:types && npm run build:bundle",
		"build:bundle": "esbuild ./src/*.ts --bundle --platform=node --format=esm --outdir=dist --allow-overwrite --target=node16",
